<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AboutPage">
        <attr name="colorCardBackground" format="color|reference" />
        <attr name="colorItemDivider" format="color|reference" />>
    </declare-styleable>

    <declare-styleable name="BaseSlidingTabsView">
        <attr name="slider_tabTitleColor" format="color"/>
        <attr name="slider_tabTitleTextSize" format="dimension"/>
        <attr name="slider_tabTitlePadding" format="dimension"/>
        <attr name="slider_tabBackgroundColor" format="color"/>
        <attr name="slider_tabIndicatorColor" format="color"/>
        <attr name="slider_tabDividerColor" format="color"/>
        <attr name="slider_contentBackgroundColor" format="color"/>
        <attr name="slider_offscreenPageLimit" format="integer"/>
        <attr name="slider_primaryTabIndex" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>
</resources>